#!/bin/bash

#input="simlarge simmedium"
input="simmedium"

# Run Scalar
bash parsecmgmt -a run -p parsec.simd.swat -c gcc-hooks -i ${input}

# Run SSE
bash parsecmgmt -a run -p parsec.simd.swat -c gcc-sse-hooks -i ${input}

# Run AVX
bash parsecmgmt -a run -p parsec.simd.swat -c gcc-avx2-hooks -i ${input}
