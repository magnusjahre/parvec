#!/bin/bash

# Run Scalar
bash parsecmgmt -a run -p parsec.simd.nbody -c gcc-hooks -i simsmall
bash parsecmgmt -a run -p parsec.simd.nbody -c gcc-hooks -i simmedium
bash parsecmgmt -a run -p parsec.simd.nbody -c gcc-hooks -i simlarge

# Run SSE
bash parsecmgmt -a run -p parsec.simd.nbody -c gcc-sse-hooks -i simsmall
bash parsecmgmt -a run -p parsec.simd.nbody -c gcc-sse-hooks -i simmedium
bash parsecmgmt -a run -p parsec.simd.nbody -c gcc-sse-hooks -i simlarge

# Run AVX
bash parsecmgmt -a run -p parsec.simd.nbody -c gcc-avx-hooks -i simsmall
bash parsecmgmt -a run -p parsec.simd.nbody -c gcc-avx-hooks -i simmedium
bash parsecmgmt -a run -p parsec.simd.nbody -c gcc-avx-hooks -i simlarge



